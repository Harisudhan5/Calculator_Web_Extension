{
    "manifest_version": 3,
    "name": "Calculator Extension",
    "version": "1.0",
    "description": "A simple calculator extension for your browser.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/calculator-16.png",
        "48": "icons/calculator-48.png",
        "128": "icons/calculator-128.png"
      }
    },
    "icons": {
      "16": "icons/calculator-16.png",
      "48": "icons/calculator-48.png",
      "128": "icons/calculator-128.png"
    },
    "permissions": []
  }
  